{
    "name":"My First Extension",
    "description": "Shows a menu item",
    "version": "0.01",
    "permissions": ["contextMenus","activeTab"],
   
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["*://www.google.com/*"], 
            "js": ["index.js"]
        }
    ],
    "manifest_version": 2
}